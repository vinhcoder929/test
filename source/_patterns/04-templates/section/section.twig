<div class="container-fluid">
    {% set header = section.header %}
    {% include "organisms-header" %}
    {% set figure = section.header.figure %}
    {% include "molecules-figure" %}
    <!-- header section-->
</div>

<div  class="container">
    <div class="text-xs-center">
        {% set item = section_02.count %}
        {% include "molecules-count" %}
    </div>
    {% set info = section_02.info %}
    {% include "organisms-info" with { includeFile: "molecules-infobox-01" } %}
    <!-- section 02 -->
    <div class="text-xs-center">
        {% include "atoms-title" with {item: section_03.title } %}
        <div class="h1">
            {% for item in section_03.count %}
                {% include "molecules-count"  %}
            {% endfor %}
        </div>
        <!--section 03-->
        {% include "atoms-title" with {item: section_04.title} %}
        {% set swiper = section_04.swiper %}
        {% include "organisms-swiper" with {includeFile:"molecules-infobox-01" , pagination: "true" } %}
        <!--section 04-->
        {% include "molecules-quote" with {item: section_05.quote} %}
        <div class="row">
        <div class="col-lg-12 pull-lg-2">
            {% set swiper = section_05.swiper %}
            {% include "organisms-swiper" with { includeFile:"molecules-infobox-01" } %}
        </div>

        <div class="col-lg-12 push-lg-2">
            {% set swiper = section_05.swiper_02 %}
            {% include "organisms-swiper" with { includeFile:"molecules-infobox-01" } %}
        </div>
    </div>
        <!--section 05-->
    </div>

    <div class="row">
        <div class="col-lg-4">
            {% set item = section_06.info_01 %}
            {% include "molecules-infobox-01" %}
        </div>
        <div class="col-lg-8">
            {% set info = section_06.info_02 %}
            {% include "organisms-info" with { includeFile: "molecules-infobox-01"} %}
        </div>
    </div>


</div>


